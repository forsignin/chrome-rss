{
    "manifest_version": 3,
    "name": "RSS Reader",
    "version": "1.1.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "src/background/service-worker.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "alarms"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}